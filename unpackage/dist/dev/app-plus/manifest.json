{
  "@platforms": [
    "android",
    "iPhone",
    "iPad"
  ],
  "id": "__UNI__E32ADC9",
  "name": "农业价格查询",
  "version": {
    "name": "1.0.0",
    "code": 1
  },
  "description": "实时查询全国农产品批发价格,粮油产品批发价格指数,农产品批发价格200指数,全国油价查询",
  "developer": {
    "name": "",
    "email": "",
    "url": ""
  },
  "permissions": {
    "UniNView": {
      "description": "UniNView原生渲染"
    }
  },
  "plus": {
    "useragent": {
      "value": "uni-app",
      "concatenate": true
    },
    "splashscreen": {
      "target": "id:1",
      "autoclose": true,
      "waiting": true,
      "delay": 0
    },
    "popGesture": "close",
    "launchwebview": {
      "id": "1",
      "kernel": "WKWebview"
    },
    "usingComponents": true,
    "nvueStyleCompiler": "uni-app",
    "compilerVersion": 3,
    "distribute": {
      "icons": {
        "android": {
          "hdpi": "static/images/icon/icon_72.png",
          "xhdpi": "static/images/icon/icon_96.png",
          "xxhdpi": "static/images/icon/icon_144.png",
          "xxxhdpi": "static/images/icon/icon_192.png"
        },
        "ios": {
          "appstore": "static/images/icon/icon_1024.png"
        }
      },
      "splashscreen": {
        "androidStyle": "default",
        "iosStyle": "common"
      },
      "google": {
        "permissions": [],
        "abiFilters": [
          "armeabi-v7a",
          "arm64-v8a"
        ],
        "minSdkVersion": 21
      },
      "apple": {
        "dSYMs": false,
        "idfa": false
      },
      "plugins": {
        "ad": {},
        "audio": {
          "mp3": {
            "description": "Android平台录音支持MP3格式文件"
          }
        }
      }
    },
    "statusbar": {
      "immersed": "supportedDevice",
      "style": "dark",
      "background": "#F8F8F8"
    },
    "uniStatistics": {
      "enable": false
    },
    "allowsInlineMediaPlayback": true,
    "safearea": {
      "background": "#ffffff",
      "bottom": {
        "offset": "auto"
      }
    },
    "uni-app": {
      "control": "uni-v3",
      "vueVersion": "3",
      "compilerVersion": "4.24",
      "nvueCompiler": "uni-app",
      "renderer": "auto",
      "nvue": {
        "flex-direction": "column"
      },
      "nvueLaunchMode": "normal",
      "webView": {
        "minUserAgentVersion": "49.0"
      }
    },
    "tabBar": {
      "position": "bottom",
      "color": "#000000",
      "selectedColor": "#000000",
      "borderStyle": "rgba(255,255,255,0.4)",
      "blurEffect": "none",
      "fontSize": "10px",
      "iconWidth": "24px",
      "spacing": "3px",
      "height": "50px",
      "backgroundColor": "#ffffff",
      "list": [
        {
          "pagePath": "pages/home/index",
          "iconPath": "/static/images/tabbar/home.png",
          "selectedIconPath": "/static/images/tabbar/home_select.png",
          "text": "农产品价格查询"
        },
        {
          "pagePath": "pages/sinopec/index",
          "iconPath": "/static/images/tabbar/oil_price.png",
          "selectedIconPath": "/static/images/tabbar/oil_price_select.png",
          "text": "全国油价"
        },
        {
          "pagePath": "pages/about/index",
          "iconPath": "/static/images/tabbar/about.png",
          "selectedIconPath": "/static/images/tabbar/about_select.png",
          "text": "关于"
        }
      ],
      "selectedIndex": 0,
      "shown": true,
      "child": [
        "lauchwebview"
      ],
      "selected": 0
    }
  },
  "launch_path": "__uniappview.html"
}